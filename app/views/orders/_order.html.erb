<tr>
    <td>
        <%= link_to 'Edit', edit_order_path(order) %>
        <%= link_to 'Show', order_path(order) %>
        <%= link_to 'Delete', order_path(order), data: { turbo_method: :delete, turbo_confirm: 'Are you sure?' } %>
    </td>
    <td><%= order.phone %></td>
    <td><%= order.name %></td>
    <td><%= order.delivery_date %></td>
    <td><%= order.delivery_time %></td>
    <td><%= order.items %></td>
    <td><%= order.address %></td>
    <td><%= order.location %></td>
    <td><%= order.total %></td>
    <td><%= order.status %></td>
    <td><%= order.created_at %></td>
</tr>